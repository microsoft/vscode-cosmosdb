  "publisher": "ms-azuretools",
  "displayName": "Azure Cosmos DB",
  "description": "Connect Azure CosmosDB databases in Azure, inspect and edit your data and run powerful queries with the visual query editor.",
  "engines": {
    "vscode": "^1.96.4",
    "node": ">=20.18.0"
  },
  "galleryBanner": {
    "color": "#3c3c3c",
    "theme": "dark"
  },
